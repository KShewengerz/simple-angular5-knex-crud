{
  "name": "simple-knex-pg-crud",
  "version": "1.0.5",
  "description": "Simple KnexJS & PostgreSQL CRUD implementation with Angular5 and Node Express",
  "main": "index.js",
  "scripts": {
    "build": "npm install && cd ./public/app && npm install",
    "undo:build": "rm -R node_modules && cd public/app && rm -R node_modules",
    "ng-start": "cd ./public/app && tsc && npm start",
    "express-start": "tsc-watch --onSuccess 'nodemon ./bin/www'",
    "start": "concurrently \"npm run express-start\"  \"npm run ng-start\"",
    "knex:latest": "babel-node --extensions '.ts' node_modules/.bin/knex migrate:latest",
    "knex:rollback": "babel-node --extensions '.ts' node_modules/.bin/knex migrate:rollback"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.17.1",
    "camelcase-keys": "^4.2.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "knex": "^0.14.6",
    "morgan": "~1.8.1",
    "pg": "^7.4.3",
    "serve-favicon": "~2.4.2",
    "snakecase-keys": "^1.1.1",
    "ts-node": "^6.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@babel/cli": "^7.0.0-beta.45",
    "@babel/core": "^7.0.0-beta.45",
    "@babel/node": "^7.0.0-beta.45",
    "@babel/preset-env": "^7.0.0-beta.45",
    "@babel/preset-typescript": "^7.0.0-beta.45",
    "@babel/register": "^7.0.0-beta.45",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/jasmine": "^2.8.7",
    "@types/knex": "^0.14.14",
    "@types/morgan": "^1.7.35",
    "@types/node": "^7.0.60",
    "@types/passport": "^0.4.5",
    "@types/passport-google-oauth": "^1.0.38",
    "@types/uuid": "^3.4.3",
    "concurrently": "^3.5.0",
    "nodemon": "^1.17.4",
    "tsc-watch": "^1.0.21",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "typescript-register": "^1.1.0"
  }
}
